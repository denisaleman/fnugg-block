{
	"name": "denisaleman/gutenberg-fnugg-whether-block",
	"description": "Provides a Gutenberg block presents data from a ski resort using the Fnugg API",
	"version": "0.0.0",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"type": "wordpress-plugin",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Dekode\\": "src/"
		}
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
		"wp-coding-standards/wpcs": "*",
		"automattic/phpcs-neutron-ruleset": "^3.3",
		"phpcompatibility/phpcompatibility-wp": "*"
	},
	"require": {
		"php": ">=7.1"
	},
	"scripts": {
		"cs": "phpcs --standard=phpcs.xml",
		"cbf": "phpcbf --standard=phpcs.xml"
	},
	"authors": [
		{
			"name": "Denis Alem√°n",
			"email": "barmaglot7@gmail.com"
		}
	]
